/* Includes ------------------------------------------------------------------*/
#include "stm32f407xx.h"

int main(){

	RCC->CR |=  RCC_CR_HSEON; //turn on ext. clock
	RCC->AHB1ENR |= RCC_AHB1ENR_GPIOAEN; // turn on clock on port A

